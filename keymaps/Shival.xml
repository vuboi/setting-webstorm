<keymap version="1" name="Shival" parent="$default">
  <action id="$LRU">
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="alt k" second-keystroke="alt w" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="Editor LRU">
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="EditorDuplicate" />
  <action id="EditorDuplicateLines">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorSelectSingleLineAtCaret">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="NewFile">
    <keyboard-shortcut first-keystroke="ctrl f9" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="alt equals" />
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="alt k" second-keystroke="alt o" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="Terminal.SplitVertically">
    <keyboard-shortcut first-keystroke="shift alt 0" />
  </action>
</keymap>